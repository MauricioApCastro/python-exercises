lista_numeros = []

for i in range(5):
    numero = int(input("Digite um número: "))
    lista_numeros.append(numero)

maior = lista_numeros[0]
menor = lista_numeros[0]

for numero in lista_numeros:
    if numero > maior:
        maior = numero
    if numero < menor:
        menor = numero

print(f"O número maior é {maior} e o número menor é {menor}")
